<div class="project-meta col-1">

<div class="info">
	<h3>informations</h3>

	<div class="date_created date">created: <span><%= moment(project.date_created).format("DD.MM.YYYY") %></span></div>
	<div class="date_updated date">updated: <span><%= moment(project.date_updated).format("DD.MM.YYYY") %></span></div>
</div>

<div class="script" id="script">

	<h3>queued scripts</h3>
	
	<div class="script">
		<div class="name-script"></div>
		<div class="queued-scripts">
		<ul class="data">			
		</ul>
		</div>
	</div>
	
</div><!-- /project-script -->

<div class="members" id="members">
    <div id="user-icon"></div>
	<h3>participants</h3>

	<% _.each(project.members, function(member){ %>
	<%= templates.Project_member({ member : member, templates : templates }) %>
	<% }); %>



	<h4><a class="button" id='link' href='#login' title='Login'> invite new members</a></h4>
	<div id='panel'>

	<form id="add-member">
		<input type="text" value="type user name"></input>
	</form>
	</div>

</div>


</div><!-- /project-meta -->

<div class="col-main">

<div id="add-element">

<div id="select-type">
	<p>add new
	<button>analysis</button>
	<button>document</button>
	<button>message</button>
	</p>
</div

<form rel="<%= project.id %>">
<textarea></textarea>

<button class="add">add</add>
</form>

</div>

<div id="elements" class="project elements">

<% _.each(elements, function(e){ %>
<%= e %>
<% }); %>

</div><!-- /elements -->

</div><!-- /col-main -->

