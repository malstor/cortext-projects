<div class="project-meta col-1">

<div class="info">
	<h3>informations</h3>

	<div class="date_created date">created: <span><%= moment(project.date_created).format("DD.MM.YYYY") %></span></div>
	<div class="date_updated date">updated: <span><%= moment(project.date_updated).format("DD.MM.YYYY") %></span></div>
</div>

<div class="script" id="script">

	<h3>queued scripts</h3>
	
	<div class="script">
		<div class="name-script"></div>
		<div class="queued-scripts">
		<ul class="data">			
		</ul>
		</div>
	</div>
	
</div><!-- /project-script -->

<div class="members" id="members">
    <div id="user-icon"></div>
	<h3>participants</h3>

	<% _.each(project.members, function(member){ %>
	<div class="member" rel="user-<%= member.id %>">
		<div class="name"><%= member.name %></div>
		<div class="participation">
		<ul class="data">
			<% _.each(member.participation, function(v, k){ %>
				<div class="<%= k %>"><%= k %>: <span><%= v %></span></div>
			<% }); %>
		</ul>
		</div>
	</div>
	<% }); %>


	<h4><a id="link" class="button" href="#"> invit for share</a></h4>
	<div id="panel">
	<!--<form id="add-member">
		<input type="text" />
		<button class="add">add</button>
	</form>-->
	</div>
	
</div>


</div><!-- /project-meta -->

<div class="col-main">

<div id="add-element">

<div id="select-type">
	<p>add new
	<button>analysis</button>
	<button>document</button>
	<button>message</button>
	</p>
</div

<form rel="<%= project.id %>">
<textarea></textarea>

<button class="add">add</add>
</form>

</div>

<div id="elements" class="project elements">

<% _.each(elements, function(e){ %>
<%= e %>
<% }); %>

</div><!-- /elements -->

</div><!-- /col-main -->

<div class="project-meta2 col-2">

<div class="help">
	<a class="info" href="http://docs.cortext.net/">
	<img src="/assets/server/images/robot1.png" alt="#" />
	<h2>you need help ?</h2>
	<h4>read documentation</h4>
	<span>Continually evisculate corporate infrastructures parallel clouded : 
	http://docs.cortext.net </span>
	</a>
</div>

</div><!-- /project-meta2 -->
