<div id="dashboard-meta" class="col-1">

<div class="info">
	<h3>informations</h3>

</div>

<div class="dashboard-projects">
	<h3>projects</h3>
	
<ul class="projects">
	<% _.each(projects, function(p){ %>
	<li class="project-title">
		<a href="/project/<%= p.id %>"><%= p.title %></a>
		<%= templates.participation({ composition: p.members[current_user_id].participation }) %>
	</li>
	<% }); %>
</ul>

</div><!-- /dashboard-projects -->
</div><!-- /dashboard-meta -->

<div id="dashboard" class="projects-list col-main">

<form id="project-new">
	<input type="text"></input>
	<button class="new">new</button>
</form>

<% _.each(projects, function(p){ %>
<div id="project-<%= p.id %>" class="project">

	<h2><a href="/project/<%= p.id %>"><%= p.title %></a>></h2>

	<%= templates.participation({ composition: p.composition }) %>

	<div class="elements">
	<% _.each(p.elements, function(e){ %>
	<div id="<%= e.type%>-<%= e.id%>" class="<%= e.type%>">
	<div class="author"><%= p.members[e.author].name %></div>
	<div class="title"><a href="/project/<%= p.id %>#<%= e.type%>-<%= e.id%>"><%= (e.title ? e.title : "lorem ipsum dolor bacon frites") %></a></div>
	<div class="date"><%= moment(e.date).format("DD.MM.YYYY") %></div>
	</div>
	<% }); %>
	</div><!-- /elements-->
</div><!-- /#project-<%= p.id %> -->
<% }); %>
</div><!-- /dashboard-meta -->

<div class="project-meta2 col-2">

<div class="help">
	<a class="info" href="http://docs.cortext.net/">
	<img src="/assets/server/images/robot1.png" alt="#" />
	<h2>you need help ?</h2>
	<h4>read documentation</h4>
	<span>Continually evisculate corporate infrastructures parallel cloud services: 
	http://docs.cortext.net </span>
	</a>
</div>

</div><!-- /dashboard-meta2 -->
