<div id="dashboard-meta" class="col-1">

<div class="info">
	<h3>informations</h3>

</div>

<div class="script" id="script">

	<h3>queued scripts</h3>
	
	<div class="script">
		<div class="name-script"></div>
		<div class="queued-scripts">
		<ul class="data">			
		</ul>
		</div>
	</div>
	
</div><!-- /project-script -->

<div class="dashboard-projects">
	<h3>projects</h3>
	
<ul class="projects">
	<% _.each(projects, function(p){ %>
	<li class="project-title">
		<a href="/project/<%= p.id %>"><%= p.title %></a>
		<span class="members-count"><%= _.size(p.members) %></span>
		<%= templates.participation({ composition: p.members[current_user_id].participation }) %>
	</li>
	<% }); %>
</ul>

</div><!-- /dashboard-projects -->
	
</div><!-- /dashboard-script -->


</div><!-- /dashboard-meta -->

<div id="dashboard" class="col-main">

<form id="project-new">
	<input type="text" value="type the name of your new project"></input>
	<button class="new">create</button>
</form>

<div class="projects-list">
<% _.each(projects, function(p){ %>
<%= templates.Dashboard_list_project( { p : p, templates : templates } ) %>
<% }); %>
</div><!-- /#dashboard -->
</div>

<div class="project-meta2 col-2">



</div><!-- /dashboard-meta2 -->
