<div id="dashboard-meta" class="col-1">

<div class="info">
	<h3>informations</h3>

</div>

<div class="dashboard-projects">
    <div id="user-icon"></div>
	<h1>projects</h1>
	
<ul class="projects">
	<% _.each(projects, function(p){ %>
	<li class="project-title">
		<a href="/project/<%= p.info.id %>"><%= p.info.title %></a>
		<div class="participation">
		<ul class="data">
			<% _.each(p.composition, function(v, k){ %>
				<div class="<%= k %>"><%= k %>: <span><%= v %></span></div>
			<% }); %>
		</ul>
		</div>
	</li>
	<% }); %>
</ul>

</div><!-- /dashboard-projects -->
</div><!-- /dashboard-meta -->

<div id="dashboard" class="projects-list">
<% _.each(projects, function(p){ %>
<div id="project-<%= p.info.id %>" class="project">

	<h2><a href="/project/<%= p.info.id %>"><%= p.info.title %></a>></h2>

	<div class="participation">
	<ul class="data">
		<% _.each(p.composition, function(v, k){ %>
			<div class="<%= k %>"><%= k %>: <span><%= v %></span></div>
		<% }); %>
	</ul>
	</div>

	<div class="elements">
	<% _.each(p.elements, function(e){ %>
	<div id="<%= e.type%>-<%= e.id%>" class="<%= e.type%>">
	<div class="author"><%= e.author.name %></div>
	<div class="title"><a href="/project/<%= p.info.id %>#<%= e.type%>-<%= e.id%>"><%= (e.title ? e.title : "lorem ipsum dolor bacon frites") %></a></div>
	<div class="date"><%= e.date_formated%></div>
	</div>
	<% }); %>
	</div><!-- /elements-->
</div><!-- /#project-<%= p.info.id %> -->
<% }); %>
</div><!-- /dashboard-meta -->